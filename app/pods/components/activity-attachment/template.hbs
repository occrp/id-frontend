<div class="activity-headline">
  <span class="iconContainer"><i class="fa fa-cloud-upload" /></span>
  <strong>{{data.user.displayName}}</strong>
  <a href="#">{{t 'activity.attachment.action'}}</a>
  {{t 'prefixes.at'}}
  <time datetime={{data.createdAt}} title={{moment-format data.createdAt 'LLL'}}>
    {{moment-format data.createdAt 'h:mm A, DD MMM YYYY'}}
  </time>
</div>